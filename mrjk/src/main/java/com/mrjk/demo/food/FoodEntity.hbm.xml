<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="com.mrjk.demo.food.FoodEntity" table="food" schema="dh">
        <id name="id" column="id"/>
        <property name="name" column="name"/>
        <property name="alias" column="alias"/>
        <property name="calorie" column="calorie"/>
        <property name="detailUrl" column="detail_url"/>
        <property name="index" column="index"/>
        <property name="addTime" column="add_time"/>
    </class>
</hibernate-mapping>